{
  "name": "generate-react",
  "version": "0.0.1",
  "description": "A command line tool for React",
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "rg": "./src/index.js"
  },
  "scripts": {
    "dev": "node ./src/index.js",
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "CLI",
    "Node.js",
    "React.js"
  ],
  "author": "Martiks38 <cappa.giuliano@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^10.0.0",
    "inquirer": "^9.1.5"
  },
  "devDependencies": {
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.6"
  }
}
